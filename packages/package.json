{
  "name": "@vakao-ui/packages",
  "version": "0.0.0",
  "description": "Vakao UI 包管理配置",
  "private": true,
  "main": "./index.ts",
  "types": "./types/index.ts",
  "exports": {
    ".": {
      "types": "./types/index.ts",
      "import": "./index.ts"
    },
    "./style.scss": "./styles/index.scss"
