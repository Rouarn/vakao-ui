    "utils",
    "utilities",
    "helpers",
    "ui-library",
    "typescript",
    "vakao-ui"
  ],
  "author": "Vakao UI Team",
  "license": "MIT",
  "homepage": "https://github.com/vakao-ui/vakao-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vakao-ui/vakao-ui.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/vakao-ui/vakao-ui/issues"
