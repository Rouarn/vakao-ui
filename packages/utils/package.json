  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0",
