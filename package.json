{
  "name": "vakao-ui",
  "version": "0.0.1",
  "description": "基于 Vue 3 和 TypeScript 的现代化组件库",
  "main": "dist/vakao-ui.umd.js",
  "module": "dist/vakao-ui.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vakao-ui.es.js",
      "require": "./dist/vakao-ui.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./resolver": {
      "types": "./dist/types/resolver.d.ts",
      "import": "./dist/resolver.es.js",
      "require": "./dist/resolver.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm -C docs dev",
    "dev:examples": "pnpm -C examples dev",
    "build": "pnpm -C packages build",
    "build:utils": "pnpm -C packages/utils build",
    "build:hooks": "pnpm -C packages/hooks build",
    "build:docs": "pnpm -C docs build",
    "build:examples": "pnpm -C examples build",
    "build:deps": "pnpm -C packages/utils build && pnpm -C packages/hooks build",
    "generate:colors": "tsx ./packages/utils/modules/generate-color.ts",
    "deploy:docs": "node ./scripts/publish.js --deploy-only --deploy-strategy docs",
    "deploy:github-pages": "node ./scripts/publish.js --deploy-only --deploy-strategy github-pages",
    "deploy:preview": "pnpm run build:docs && pnpm -C docs preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{vue,js,ts,json,md}\"",
    "publish": "node ./scripts/publish.js",
    "publish:help": "node ./scripts/publish.js --help",
    "publish:dry-run": "node ./scripts/publish.js --dry-run",
    "publish:main": "node ./scripts/publish.js --package main",
    "publish:main:dry-run": "node ./scripts/publish.js --package main --dry-run",
    "publish:hooks": "node ./scripts/publish.js --package hooks",
    "publish:hooks:dry-run": "node ./scripts/publish.js --package hooks --dry-run",
    "publish:utils": "node ./scripts/publish.js --package utils",
    "publish:utils:dry-run": "node ./scripts/publish.js --package utils --dry-run",
    "deploy:only": "node ./scripts/publish.js --deploy-only",
    "deploy:interactive": "node ./scripts/publish.js --deploy-only",
    "publish:all": "node ./scripts/publish.js --packages main,hooks,utils",
    "publish:all:dry-run": "node ./scripts/publish.js --packages main,hooks,utils --dry-run",
    "publish:sync": "node ./scripts/publish.js --sync-version",
    "publish:sync:dry-run": "node ./scripts/publish.js --sync-version --dry-run",
    "test:notification": "node -e \"const notifier = require('node-notifier'); notifier.notify({title: '测试通知', message: '桌面通知功能正常', sound: true});\"",
    "prepare": "husky",
    "commit": "git add -A && czg && git push"
  },
  "keywords": [
    "vue3",
    "components",
    "ui-library",
    "typescript"
  ],
  "author": "我与夏季",
  "email": "woyuxiaji@foxmail.com",
  "homepage": "https://rouarn.github.io/vakao-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rouarn/vakao-ui"
  },
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@iconify/vue": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vakao-ui/components": "workspace:*",
    "@vakao-ui/hooks": "workspace:*",
    "@vakao-ui/utils": "workspace:*",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "czg": "1.12.0",
    "eslint": "^9.32.0",
    "eslint-plugin-vue": "^9.33.0",
    "fs-extra": "^11.3.1",
    "husky": "9.1.7",
    "inquirer": "^9.3.7",
    "lint-staged": "16.1.5",
    "node-notifier": "^10.0.1",
    "prettier": "^3.6.2",
    "terser": "^5.43.1",
    "tsx": "4.20.3",
    "typescript": "^5.9.2",
    "unplugin-vue-components": "^28.8.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "pnpm": ">=7.0.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
