{
  "name": "vakao-ui",
  "version": "1.0.21",
  "description": "A Vue 3 component library based on NaiveUI",
  "main": "dist/vakao-ui.umd.js",
  "module": "dist/vakao-ui.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vakao-ui.es.js",
      "require": "./dist/vakao-ui.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./resolver": {
      "import": "./dist/vakao-ui.es.js",
      "require": "./dist/vakao-ui.umd.js",
      "types": "./dist/types/resolver.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm -C docs dev",
    "build": "pnpm -C packages build",
    "build:docs": "pnpm -C docs build",
    "prepare-dist": "node ./scripts/prepare-dist.js",
    "deploy:docs": "node ./scripts/deploy.js",
    "deploy:preview": "pnpm run build:docs && pnpm -C docs serve",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{vue,js,ts,json,md}\"",
    "publish:only": "node ./scripts/publish.js",
    "publish:dry-run": "node ./scripts/publish.js --dry-run"
  },
  "keywords": [
    "vue3",
    "components",
    "ui-library",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vakao-ui/components": "workspace:*",
    "@vakao-ui/hooks": "workspace:*",
    "@vakao-ui/utils": "workspace:*",
    "@vicons/ionicons5": "^0.13.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "fs-extra": "^11.3.0",
    "prettier": "^3.0.0",
    "terser": "^5.43.1",
    "typescript": "^5.1.6",
    "unplugin-vue-components": "^28.8.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}